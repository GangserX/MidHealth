{
  "name": "@midhealth/contract",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Compact smart contract for healthcare attestation on Midnight",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compact compile src/PatientCredential.compact ./src/managed/patient-credential",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -Rf ./src/managed ./dist/managed && cp ./src/PatientCredential.compact ./dist/",
    "clean": "rm -rf dist src/managed",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "deploy": "node dist/deploy.js"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
